/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Nand(a=sel, b=sel, out=notsel);
	Nand(a=notsel, b=a, out=nandnsela);
	Nand(a=sel, b=b, out=nandselb);
	Nand(a=nandnsela, b=nandselb, out=out);
}
